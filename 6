SELECT meal_name, COUNT(order_id) AS order_count
FROM meal_orders
GROUP BY meal_name;

SELECT SUM(price) AS total_revenue
FROM meal_orders
WHERE price > 15;

SELECT COUNT(*) AS high_calorie_meals_count
FROM meal_orders
WHERE calories > 500;

SELECT SUM(price) AS total_price
FROM meal_orders
WHERE LOWER(meal_name) LIKE '%s%';

SELECT meal_name, COUNT(order_id) AS order_count
FROM meal_orders
WHERE order_date < '2023-10-07 18:40:00'
GROUP BY meal_name;
